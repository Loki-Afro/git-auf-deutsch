[project]
	description = Git auf deutsch
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Phillip (Loki-Afro)
[access "refs/for/refs/*"]
	read = group user/Phillip (Loki-Afro)
	push = group user/Phillip (Loki-Afro)
	label-Code-Review = -2..+2 group user/Phillip (Loki-Afro)
	label-Verified = -1..+1 group user/Phillip (Loki-Afro)
	rebase = group user/Phillip (Loki-Afro)
	removeReviewer = group user/Phillip (Loki-Afro)
	submit = group user/Phillip (Loki-Afro)
[access "refs/heads/*"]
	read = group user/Phillip (Loki-Afro)
	create = group user/Phillip (Loki-Afro)
	push = +force group user/Phillip (Loki-Afro)
	pushMerge = group user/Phillip (Loki-Afro)
[access "refs/tags/*"]
	push = group user/Phillip (Loki-Afro)
	push = +force group user/Phillip (Loki-Afro)
